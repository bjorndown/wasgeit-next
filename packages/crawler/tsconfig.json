  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "noEmit": true,
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "lib": ["es2021", "dom"]
  }
}
